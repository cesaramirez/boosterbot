        "bulma-extensions": "^2.2.1",
        "cross-env": "^5.2.0",
        "laravel-mix": "^2.1.11",
        "lodash": "^4.17.10",
        "tailwindcss": "^0.6.1",
        "vue": "^2.5.16"
    }
